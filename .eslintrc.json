{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-multiple-empty-lines":"off",
        "no-trailing-spaces":"off",
        "padded-blocks":"off",
        "prefer-const":"warn",
        "no-unused-vars":"warn",
        "no-empty":"off",
        "@typescript-eslint/no-unused-vars":"warn",
        "@typescript-eslint/no-non-null-assertion":"off"
    }
}
